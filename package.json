{
  "name": "rewrite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter=playground dev",
    "build": "pnpm --filter=0 --filter=paper build",
    "preview": "vite preview",
    "type-check": "vue-tsc -b --incremental",
    "type-check:watch": "vue-tsc -b --incremental --watch",
    "type-check:clean": "vue-tsc -b --clean && vue-tsc -b --incremental",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "storybook": "pnpm --filter=storybook dev",
    "build:storybook": "pnpm --filter=storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:bench": "vitest run --reporter=verbose bench",
    "bench": "vitest run --reporter=default bench",
    "coverage": "vitest run --coverage",
    "dev:docs": "pnpm --filter=docs dev",
    "build:docs": "pnpm --filter=docs build"
  },
  "engines": {
    "node": ">=20.19 || >=22",
    "pnpm": ">=10.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "^24.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/tsconfig": "catalog:",
    "eslint": "^9.29.0",
    "eslint-config-vuetify": "4.1.0-0",
    "eslint-plugin-storybook": "^9.0.13",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^18.0.1",
    "typescript": "catalog:",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.12.4"
}
