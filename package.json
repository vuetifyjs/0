{
  "name": "@vuetify/zero",
  "version": "0.0.9",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter=playground dev",
    "build": "pnpm --filter=\"./packages/*\" build",
    "preview": "vite preview",
    "typecheck": "pnpm --filter=\"./packages/0\" typecheck",
    "lint": "eslint --concurrency=4",
    "lint:fix": "eslint --concurrency=4 --fix",
    "storybook": "pnpm --filter=storybook dev",
    "build:storybook": "pnpm --filter=storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:bench": "vitest --reporter=verbose bench --run",
    "test:knip": "knip",
    "test:sherif": "sherif",
    "bench": "vitest --reporter=default bench",
    "coverage": "vitest run --coverage",
    "dev:docs": "pnpm --filter=docs dev",
    "build:docs": "pnpm --filter=docs build",
    "release": "bumpp -r"
  },
  "engines": {
    "node": ">=20.19 || >=22",
    "pnpm": ">=10.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vue/tsconfig": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "eslint-config-vuetify": "catalog:",
    "eslint-plugin-storybook": "catalog:",
    "eslint-plugin-vuejs-accessibility": "catalog:",
    "happy-dom": "catalog:",
    "knip": "catalog:",
    "sherif": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "packageManager": "pnpm@10.18.2"
}
